<div class="conteiner">
  <div class="row">
    <div class="col-12 mt-5 mb-5">
      <table class="table">
        <theard>
          <tr>
            <th>会員ID</th>
            <th>氏名</th>
            <th>メールアドレス</th>
            <th>ステータス</th>
          </tr>
        </theard>
        <% @users.each do |user| %>
        <tbody>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= user.is_active %></td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>